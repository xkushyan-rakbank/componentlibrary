{
  "name": "@xkushyam-rakbank/componentlibrary",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "keywords": [],
  "author": "Raja Osama",
  "description": "An Reactive Atomic Design System Scafold for creating a design system",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint 'src/**/*.{ts,tsx,css}'",
    "prepare": "husky install",
    "test": "jest",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/reacting-atomic-desing-system.es.js",
      "require": "./dist/reacting-atomic-desing-system.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/reacting-atomic-desing-system.umd.js",
  "module": "./dist/reacting-atomic-desing-system.es.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@fontsource/cabin": "^5.0.17",
    "@fontsource/material-icons": "^5.0.14",
    "@fontsource/roboto": "^5.0.12",
    "@fontsource/space-grotesk": "^5.0.17",
    "@mui/icons-material": "^5.15.12",
    "@mui/material": "^5.15.12",
    "@mui/styled-engine-sc": "^6.0.0-alpha.17",
    "jest-environment-jsdom": "^29.7.0",
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x",
    "styled-components": "^6.1.8",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/addon-themes": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/storybook__react": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "react-hooks": "^1.0.1",
    "storybook": "^7.6.17",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-linter": "^2.1.1",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn  eslint",
      "yarn  format"
    ]
  }
}
